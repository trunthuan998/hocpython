    "p = turtle.Turtle()\n",
    "p.pencolor(\"red\")\n",
    "p.fillcolor(\"yellow\")\n",
    "\n",
    "for i in range(2):\n",
    " p.forward(200)\n",
    " p.right(90)\n",
    " p.forward(100)\n",
    " p.right(90)\n",
    "\n",
    "p.left(30)\n",
    "p.forward(100)\n",
    "p.right(60)\n",
    "p.forward(100)\n",
    "\n",
    "turtle.done()"
   ]
